{
  "headers": [
    {
      "source": "/:path(.*).(jpg|jpeg|png|gif|webp|svg|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, stale-while-revalidate=600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(?:jpg|jpeg|png|gif|webp|svg|ico)$).*)",
      "destination": "/api/index.js"
    }
  ]
}
