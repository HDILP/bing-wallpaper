{
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*).(jpg|jpeg|png|gif|webp|svg|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index.js"
    }
  ]
}
